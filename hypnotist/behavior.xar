	def onInput_onStart(self, p):
		p = self.typeConversion(p)
		if(p == self.typeConversion("Sit")):
			self.output_1(p)
		elif(p == self.typeConversion("Unknown")):
			self.output_2(p)
		elif(p == self.typeConversion("HeadBack")):
			self.output_3(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
				p = pint
		except:
			p = str(p)
		return p]]>
